<%if(success && success.length){%>
<div
  class="alert alert-success alert-dismissible fade show col-6 mb-0"
  role="alert"
  style="width: 100%; text-align: center; transform: translate3d(0, 0, 0)"
>
  <%-success%>
  <button
    type="button"
    class="btn-close"
    id="cross-btn"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<script></script>

<%}%> <%if(error && error.length){%>
<div
  class="alert alert-danger alert-dismissible fade show col-6 mb-0"
  role="alert"
  style="width: 100%; text-align: center; transform: translate3d(0, 0, 0)"
>
  <%-error%>
  <button
    type="button"
    class="btn-close"
    id="cross-btn"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
