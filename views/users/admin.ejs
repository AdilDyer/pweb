    <!-- This Part Of Script Is When clicked On The Anchor Tag The content Should Change -->
    <script>
      const anchors = document.querySelectorAll("ul a");
      const sectioncontent = document.getElementById("allofyou");

      anchors.forEach((anchor) => {
        anchor.addEventListener("click", (event) => {
          event.preventDefault(); // Prevent default anchor tag behavior (page jump)

          const targetId = anchor.getAttribute("href").slice(1); // Extract target ID from href attribute
          const targetSection = sectioncontent.querySelector(`#${targetId}`);

          // Hide all sections
          sectioncontent
            .querySelectorAll("section")
            .forEach((section) => (section.style.display = "none"));

          // Show the clicked section
          targetSection.style.display = "block";
        });
      });
    </script>

