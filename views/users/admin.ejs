    <script>
      document.addEventListener("DOMContentLoaded", function () {
        var adminName = "Admin"; // Replace with actual admin name
        var welcomeMessage = document.getElementById("welcome-message");

        function displayLetters(text) {
          var index = 0;
          var interval = setInterval(function () {
            if (index < text.length) {
              welcomeMessage.innerHTML += text.charAt(index);
              index++;
            } else {
              clearInterval(interval);
            }
          }, 100); // Change the interval duration as needed
        }

        displayLetters(
          "Welcome back, " + adminName + "! Glad to have you here."
        );
      });
    </script>
