              </script>
              <script>
                // Function to check if any checkbox is checked
                function anyCheckboxChecked() {
                  const checkboxes = document.querySelectorAll(
                    'input[name="studentMarkAsAuditedChecks"]'
                  );
                  for (const checkbox of checkboxes) {
                    if (checkbox.checked) {
                      return true;
                    }
                  }
                  return false;
                }

                // Add event listener to each checkbox
                document
                  .querySelectorAll('input[name="studentMarkAsAuditedChecks"]')
                  .forEach((checkbox) => {
                    checkbox.addEventListener("change", function () {
                      const submitButton = document.getElementById(
                        "markAllStudentAudited"
                      );
                      if (anyCheckboxChecked()) {
                        submitButton.style.display = "block";
                      } else {
                        submitButton.style.display = "none";
                      }
                    });
                  });
              </script>
              <script>
                document
                  .getElementById("markAllStudentAudited")
                  .addEventListener("click", function (event) {
                    event.preventDefault();

                    // const formData = new FormData(this);
                    const idsOfCheckboxes = [];

                    let allCheckedNonAuditedStudents =
                      document.querySelectorAll(
                        `input[name="studentMarkAsAuditedChecks"]:checked`
                      );

                    allCheckedNonAuditedStudents.forEach((checkbox) => {
                      idsOfCheckboxes.push(checkbox.value);
                    });

                    let form = document.createElement("form");
                    form.method = "post";
                    form.action = "/admin/markStuAudit/112233";

                    let input = document.createElement("input");
                    input.type = "hidden";
                    input.name = "idsOfCheckboxes"; // Name of the input field
                    const idsOfCheckboxesWithoutOn = idsOfCheckboxes.filter(
                      (id) => id !== "on"
                    );
                    input.value = JSON.stringify(idsOfCheckboxesWithoutOn);
                    // Convert the array to a JSON string
                    form.appendChild(input);

                    document.body.appendChild(form);
                    form.submit();
