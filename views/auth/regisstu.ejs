    <script>
      let personalDetailsSectionInputs = document.querySelectorAll(
        "#personalDetailsSection input"
      );
      let CommuDetailsSectionInputs = document.querySelectorAll(
        "#commuDetailsSection input"
      );
      let EduDetailsSectionInputs = document.querySelectorAll(
        "#eduDetailsSection input"
      );
      let DecDetailsSectionInputs = document.querySelectorAll(
        "#declarationDetailsSection"
      );
      let firstNextBtn = document.getElementById("firstNextBtn");
      let secondNextBtn = document.getElementById("secondNextBtn");
      let thirdNextBtn = document.getElementById("thirdNextBtn");
      let commuBtn = document.querySelector("#commuBtn");
      commuBtn.style.color = "black";
      let eduBtn = document.querySelector("#eduBtn");
      eduBtn.style.color = "black";
      let declaBtn = document.querySelector("#declaBtn");
      declaBtn.style.color = "black";
      document.addEventListener("DOMContentLoaded", () => {
        // Function to check if all inputs are filled
        function checkInputs1() {
          let allFilled = true;
          personalDetailsSectionInputs.forEach((input) => {
            if (input.value.trim() == "" && input.required == true) {
              allFilled = false;
            }
          });
          firstNextBtn.disabled = !allFilled;
          commuBtn.style.display = "none";
          eduBtn.style.display = "none";
          declaBtn.style.display = "none";
        }
        function checkInputs2() {
          let allFilled = true;
          CommuDetailsSectionInputs.forEach((input) => {
            if (
              input.value.trim() == "" &&
              input.type != "checkbox" &&
              input.required == true
            ) {
              allFilled = false;
              console.log(input);
            }
          });
          secondNextBtn.disabled = !allFilled;
          commuBtn.style.display = "inline";
          eduBtn.style.display = "none";
          declaBtn.style.display = "none";
        }

        function checkInputs3() {
          let allFilled = true;
          EduDetailsSectionInputs.forEach((input) => {
            if (input.value.trim() == "" && input.required == true) {
              allFilled = false;
            }
          });
          thirdNextBtn.disabled = !allFilled;
          eduBtn.style.display = "inline";
          declaBtn.style.display = "none";
        }
        function checkInputs4() {
          declaBtn.style.display = "inline";
        }

        // Initial check on page load
        checkInputs1();

        firstNextBtn.addEventListener("click", checkInputs2);
        secondNextBtn.addEventListener("click", checkInputs3);
        thirdNextBtn.addEventListener("click", checkInputs4);

        // Add event listeners to each input
        personalDetailsSectionInputs.forEach((input) => {
          input.addEventListener("input", checkInputs1);
        });
        CommuDetailsSectionInputs.forEach((input) => {
          input.addEventListener("input", checkInputs2);
        });
        EduDetailsSectionInputs.forEach((input) => {
          input.addEventListener("input", checkInputs3);
        });
      });
